<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>AKT - Architect</title>

  <!--favicon-->
  <link rel="shortcut icon" href="~/images/favicon/AKT.ico">
  <link rel="shortcut icon" href="~/images/favicon/AKT.png">
  <!--iOS-->
  <link rel="apple-touch-icon-precomposed" sizes="57x57" href="~/images/favicon/AKT-icon-57x57-precomposed.png" />
  <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/images/favicon/AKT-icon-72x72-precomposed.png" />
  <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/images/favicon/AKT-icon-114x114-precomposed.png" />
  <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/images/favicon/AKT-icon-144x144-precomposed.png" />

  <link rel="stylesheet" href="~/css/uikit.min.css" />
  <link href="/css/main.css" rel="stylesheet" />
  <link href="/css/navbar.css" rel="stylesheet" />

  <!--Google Ads-->
  <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
  <script>
    (adsbygoogle = window.adsbygoogle || []).push({
      google_ad_client: "ca-pub-5097396064168160",
      enable_page_level_ads: true
    });
  </script>
</head>
<body>
  <!--Navbar-->
  <div id="navbar">
    <div class="cd-main-header">
      <a asp-controller="Home" asp-action="Index" class="cd-logo"><img src="/images/cd-logo.svg" alt="Logo"></a>
      <a href="#0" class="cd-nav-trigger"><span></span></a>

      <nav class="cd-nav">
        <ul class="cd-top-nav">
          <li><a asp-controller="Home" asp-action="Index">Home</a></li>
          <li><a asp-controller="Search" asp-action="Index">Search</a></li>
          <li><a asp-controller="Home" asp-action="About">About</a></li>
          <li><a asp-controller="Home" asp-action="Contact">Contact</a></li>
          <li class="has-children account">
            <a href="#0">
              <img src="/images/cd-avatar.jpg" alt="avatar">
              Account
            </a>

            <ul>
              <li><a href="#0">My Account</a></li>
              <li><a href="#0">Log Out</a></li>
            </ul>
          </li>
        </ul>
      </nav>
    </div> <!-- .cd-main-header -->

    <div class="cd-main-content">
      <nav class="cd-side-nav">
        <ul>
          <li class="cd-label">Main</li>
          <li class="has-children overview">
            <a href="#0">Overview</a>

            <ul>
              <li><a href="#0">All Data</a></li>
              <!-- other list items here -->
            </ul>
          </li>
          <li class="has-children notifications active">
            <a href="#0">Notifications<span class="count">3</span></a>

            <ul>
              <li><a href="#0">All Notifications</a></li>
              <!-- other list items here -->
            </ul>
          </li>

          <!-- other list items here -->
        </ul>

        <!-- other unordered lists here -->
      </nav>

      <div class="content-wrapper">
        <!-- main content here -->
      </div> <!-- .content-wrapper -->
    </div> <!-- .cd-main-content -->
  </div>

  <!--Body content-->
  <div id="background">
    @RenderBody()
  </div>
  @*<partial name="_CookieConsentPartial" />*@

  <footer style="color: white">
    &copy; 2019 - AKT - Architect
    <!--
      <div class="uk-container uk-align-center" style="max-height: 50px; width: 100%; position: relative">
    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <!-- SideBar -->
    <!--
    <ins class="adsbygoogle"
         style="display:block"
